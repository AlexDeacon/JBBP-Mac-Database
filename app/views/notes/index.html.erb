<h1>Listing notes</h1>

<table>
  <tr>
    <th>User name</th>
    <th>User</th>
    <th>Note text</th>
    <th>Computer</th>
    <th>Battery</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.user_name %></td>
    <td><%= note.user_id %></td>
    <td><%= note.note_text %></td>
    <td><%= note.computer %></td>
    <td><%= note.battery.battery_jbbp_id %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
